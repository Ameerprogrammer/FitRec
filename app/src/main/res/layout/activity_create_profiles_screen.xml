<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CreateProfiles">

    <Button
        android:id="@+id/button"
        android:layout_width="188dp"
        android:layout_height="39dp"
        android:background="@drawable/button_outline"
        android:text="@string/text_20"
        android:textColor="#3EB489"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.39"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.628" />

    <EditText
        android:id="@+id/enterAge"
        android:layout_width="113dp"
        android:layout_height="41dp"
        android:background="@drawable/rounded_corner"
        android:hint="Age?"
        android:inputType="text"
        android:paddingLeft="35dp"
        android:paddingTop="5dp"
        android:paddingRight="5dp"
        android:paddingBottom="5dp"
        android:textColor="#3EB489"
        android:textColorHint="#3EB489"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.424"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.341"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/enterStyle"
        android:layout_width="113dp"
        android:layout_height="41dp"
        android:background="@drawable/rounded_corner"
        android:hint="Style?"
        android:inputType="text"
        android:paddingLeft="32dp"
        android:paddingTop="5dp"
        android:paddingRight="5dp"
        android:paddingBottom="5dp"
        android:textColor="#3EB489"
        android:textColorHint="#3EB489"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.422"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.482"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/enterGender"
        android:layout_width="113dp"
        android:layout_height="41dp"
        android:background="@drawable/rounded_corner"
        android:hint="Gender?"
        android:inputType="text"
        android:paddingLeft="22dp"
        android:paddingTop="5dp"
        android:paddingRight="12dp"
        android:paddingBottom="5dp"
        android:textColor="#3EB489"
        android:textColorHint="#3EB489"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.424"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.274"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/enterOccasion"
        android:layout_width="113dp"
        android:layout_height="41dp"
        android:background="@drawable/rounded_corner"
        android:hint="Occasion?"
        android:inputType="text"
        android:paddingLeft="15dp"
        android:paddingTop="5dp"
        android:paddingRight="10dp"
        android:paddingBottom="5dp"
        android:textColor="#3EB489"
        android:textColorHint="#3EB489"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.422"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.553"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/enterBodyType"
        android:layout_width="113dp"
        android:layout_height="41dp"
        android:background="@drawable/rounded_corner"
        android:hint="Body Type?"
        android:inputType="text"
        android:paddingLeft="12dp"
        android:paddingTop="5dp"
        android:paddingRight="5dp"
        android:paddingBottom="5dp"
        android:textColor="#3EB489"
        android:textColorHint="#3EB489"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.422"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.413"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="317dp"
        android:layout_height="51dp"
        android:text="@string/text_19"
        android:textColor="#3EB489"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.194" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="114dp"
        android:layout_marginEnd="114dp"
        android:text="@string/text_18"
        android:textColor="#3EB489"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.115" />
</androidx.constraintlayout.widget.ConstraintLayout>